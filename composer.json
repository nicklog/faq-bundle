{
    "name": "tenolo/faq-bundle",
    "description": "Implements faq functions.",
    "keywords": [
        "faq",
        "tenolo",
        "symfony",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",

        "symfony/framework-bundle": "~3.4|~4.0",
        "symfony/http-kernel": "~3.4|~4.0",
        "symfony/config": "~3.4|~4.0",
        "symfony/dependency-injection": "~3.4|~4.0",
        "symfony/form": "~3.4|~4.0",
        "symfony/routing": "~3.4|~4.0",
        "symfony/options-resolver": "~3.4|~4.0",
        "symfony/doctrine-bridge": "~3.4|~4.0",
        "symfony/yaml": "~3.4|~4.0",
        "symfony/twig-bundle": "~3.4|~4.0",

        "doctrine/common": "~2.6",
        "doctrine/orm": "~2.5",
        "doctrine/doctrine-bundle": "~1.6",

        "tenolo/slugify-bundle": "~1.5",
        "tenolo/entity-bundle": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\FAQBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
